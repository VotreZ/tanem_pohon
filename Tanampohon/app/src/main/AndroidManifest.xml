<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tanampohon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".TerimakasihActivity"></activity>
        <activity android:name=".RiwayatActivity"
            android:label="Riwayat Donasi"
            android:theme="@style/AppTheme"
            android:parentActivityName=".HomeActivity">
        </activity>
        <activity android:name=".DonasiActivity" />
        <activity android:name=".AkunActivity"
            android:label="Profil Akun"
            android:theme="@style/AppTheme"
            android:parentActivityName=".HomeActivity">
        </activity>
        <activity android:name=".HomeActivity"/>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".Koneksi.AkunActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>